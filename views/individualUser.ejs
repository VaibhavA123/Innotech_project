                        <a href="/service" >Create section</a>
                        <a href="/safety" >Battery_Level</a>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle"  data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">Signup/login</a>
                            <ul class="dropdown-menu" >
                            <li><a class="dropdown-item" href="/signup">Sign up</a></li>
